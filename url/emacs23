 :license    "GPL-3"
 :authors    (("Bill Perry" . "wmperry@gnu.org"))
 :maintainer ("Bill Perry" . "wmperry@gnu.org")
 :provided   (url url-about url-auth url-cache url-cookie url-dav url-dired url-expand url-file url-ftp url-gw url-handlers url-history url-http url-irc url-ldap url-mailto url-methods url-misc url-news url-nfs url-ns url-parse url-privacy url-proxy url-util url-vars)
 :required   ((("cl" cl)
	       ("emacs-core" md5)
	       ("gnus" mail-parse mailcap mm-decode mm-util mm-view nntp parse-time tls)
	       ("ldap" ldap)
	       ("twittering-mode" ssl)
	       ("xml" xml)))
 :keywords   ("comm" "data" "processes" "hypermedia")
 :commentary "\
