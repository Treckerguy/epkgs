 :provided   (predictive-prog-mode rails rails-cmd-proxy rails-compat rails-controller-layout rails-controller-minor-mode rails-core rails-features rails-find rails-fixture-minor-mode rails-functional-test-minor-mode rails-helper-minor-mode rails-layout-minor-mode rails-lib rails-log rails-mailer-minor-mode rails-migration-minor-mode rails-model-layout rails-model-minor-mode rails-navigation rails-plugin-minor-mode rails-project rails-rake rails-rspec-feature rails-ruby rails-scripts rails-snippets-feature rails-spec rails-speedbar-feature rails-test rails-ui rails-unit-test-minor-mode rails-view-minor-mode rails-ws)
 :required   ((("emacs" align ansi-color etags flyspell hideshow ruby-mode speedbar sql)
	       ("nxhtml" find-recursive)
	       ("rinari" inf-ruby)
	       ("ruby-electric" ruby-electric)
	       ("snippet" snippet)
	       (nil inflections))
	      (("completion-ui" completion-ui)
	       ("emacs" flymake)
	       ("predictive" predictive)))
