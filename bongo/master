(:summary    "Flexible and usable media player for Emacs"
 :repository (git . "git://github.com/dbrock/bongo.git")
 :created    "2005"
 :updated    "2010"
 :license    "GPL-2"
 :authors    (("Daniel Brockman" . "daniel@brockman.se"))
 :maintainer ("Daniel Brockman" . "daniel@brockman.se")
 :provided   (bongo bongo-emacs21 bongo-lastfm bongo-mplayer lastfm-submit)
 :required   ((("cl" cl)
	       ("custom" cus-edit custom)
	       ("electric" electric)
	       ("find-func" find-func)
	       ("gnus" format-spec time-date)
	       ("rx" rx))
	      (("volume" volume)))
 :homepage   "http://www.emacswiki.org/cgi-bin/wiki/Bongo"
 :wikipage   "http://www.emacswiki.org/Bongo")
