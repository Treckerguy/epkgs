 :authors    (("Noah Friedman" . "friedman@splode.com"))
 :maintainer (nil . "friedman@splode.com")
 :provided   (order-head)
 :required   ((("emacs" rfc822)
	       ("fmailutils" fmailutils)))
 :keywords   ("mail" "extensions")
