(:summary    "Caml mode for (X)Emacs"
 :license    "GPL-2"
 :provided   (ocamlspot sym-lock tuareg)
 :required   ((("cl" cl)
	       ("comint" comint)
	       ("compile" compile)
	       ("custom" custom)
	       ("easymenu" easymenu)
	       ("emacs-core" font-lock))
	      (("caml" caml-help caml-types)
	       ("cedet" speedbar)))
 :homepage   "http://tuareg.forge.ocamlcore.org/"
 :wikipage   "http://www.emacswiki.org/TuaregMode")
