(:summary    "Superior Lisp Interaction Mode for Emacs"
 :repository (git . "git://common-lisp.net/projects/mirror/slime.git")
 :license    "GPL-2"
 :provided   (bridge hyperspec inferior-slime slime slime-asdf slime-autodoc slime-autoloads slime-banner slime-c-p-c slime-clipboard slime-compiler-notes-tree slime-editing-commands slime-enclosing-context slime-fancy slime-fancy-inspector slime-fontifying-fu slime-fuzzy slime-highlight-edits slime-hyperdoc slime-indentation slime-mdot-fu slime-media slime-motd slime-mrepl slime-package-fu slime-parse slime-presentation-streams slime-presentations slime-references slime-repl slime-sbcl-exts slime-scheme slime-scratch slime-snapshot slime-sprof slime-tramp slime-typeout-frame slime-xref-browser)
 :required   ((("apropos" apropos)
	       ("arc-mode" arc-mode)
	       ("browse-url" browse-url)
	       ("cl" cl)
	       ("comint" comint)
	       ("compile" compile)
	       ("easymenu" easymenu)
	       ("elp" elp)
	       ("emacs-core" bytecomp font-lock overlay pp)
	       ("etags" etags)
	       ("gud" gud)
	       ("hideshow" hideshow)
	       ("outline" outline)
	       ("thingatpt" thingatpt)
	       ("timer" timer)
	       ("tree-widget" tree-widget)))
 :homepage   "http://common-lisp.net/project/slime"
